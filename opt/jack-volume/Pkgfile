# Description: jack volume app
# URL:         https://github.com/voidseg/jack-volume.git

name=jack-volume
version=git
# branch=master
release=1
source=(
        https://github.com/voidseg/jack-volume.git
        $name.diff
)

build()
{
    cd $name
    patch -i $SRC/$name.diff
    make
    make install DESTDIR=$PKG
}
